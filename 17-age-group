public String ageGroup(Integer n) {
    //code here
    if (n >= 22) {
        system.debug('Adult');
    }
    else if (n >= 15 && n <= 21) {
        system.debug('Youth');
    }
    else if (n >= 2 && n <= 14) {
        system.debug('Child');
    }
    else {
        system.debug('Infant');
    }
}